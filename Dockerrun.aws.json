{
  "AWSEBDockerrunVersion":2,
  "containerDefinations":[
    {
      "name": "client",
      "image": "anandahlawat90/multi-client",
      "hostname": "client",
      "essential":false
    },
    {
      "name":"server",
      "image":"anandahlawat90/multi-server",
      "hostname":"api",
      "essential":false
    },
    {
      "name":"worker",
      "image":"anandahlawat90/multi-worker",
      "hostname":"worker",
      "essential":false
    },
    {
      "name":"nginx",
      "image":"anandahlawat90/multi-nginx",
      "essential":true,
      "portMappings":[
        {
          "hostPort":80,
          "containerPort":80
        }
      ],
      "links" : ["client","server"]
    }
  ]
}
